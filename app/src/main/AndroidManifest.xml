<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.company_name.under_armour_app">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">

		<meta-data
			android:name="com.robotemi.sdk.metadata.SKILL"
			android:value="@string/app_name"/>
	
		<activity
			android:label="@string/further_assist_activity_title"
			android:name=".activity.FurtherAssistActivity"
			android:theme="@style/FurtherAssistActivityTheme"/>
	
		<activity
			android:label="@string/after_add_to_bag_activity_title"
			android:name=".activity.AfterAddToBagActivity"
			android:theme="@style/AfterAddToBagActivityTheme"/>
	
		<activity
			android:label="@string/tag_activity_title"
			android:name=".activity.TagActivity"
			android:theme="@style/TagActivityTheme"/>
	
		<activity
			android:label="@string/shop_activity_title"
			android:name=".activity.ShopActivity"
			android:theme="@style/ShopActivityTheme"/>
	
		<activity
			android:label="@string/walk_call_shop_returnactivity_title"
			android:name=".activity.WalkCallShopRETURNActivity"
			android:theme="@style/WalkCallShopRETURNActivityTheme"/>
	
		<activity
			android:label="@string/welcomewalk_call_shop_activity_title"
			android:name=".activity.WelcomewalkCallShopActivity"
			android:theme="@style/WelcomewalkCallShopActivityTheme">
		
			<intent-filter>
			
				<category android:name="android.intent.category.LAUNCHER"/>
			
				<action android:name="android.intent.action.MAIN"/>
			</intent-filter>
		</activity>
	
		<activity
			android:label="@string/walk_menu_return_activity_title"
			android:name=".activity.WalkMenuReturnActivity"
			android:theme="@style/WalkMenuReturnActivityTheme"/>
	
		<activity
			android:label="@string/initialwalk_menu_activity_title"
			android:name=".activity.InitialwalkMenuActivity"
			android:theme="@style/InitialwalkMenuActivityTheme"/>
	</application>
</manifest>