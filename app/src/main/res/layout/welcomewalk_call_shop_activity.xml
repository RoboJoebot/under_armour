<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/welcomewalkcallshop_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/welcomewalk_call_shop_activity_welcomewalkcallshop_constraint_layout_background_color">

	<ImageView
		android:id="@+id/rectangle17_image_view"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:scaleType="centerCrop"
		android:src="@drawable/rectangle_17"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintHorizontal_weight="1"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="0dp"/>

	<ImageView
		android:id="@+id/walkbutton_image_view"
		android:layout_width="441dp"
		android:layout_height="501dp"
		android:layout_marginLeft="20dp"
		android:onClick="walk"
		android:scaleType="centerCrop"
		android:src="@drawable/walkbutton"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.511" />

	<ImageView
		android:id="@+id/ua_logo1_image_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="15dp"
		android:scaleType="center"
		android:src="@drawable/ualogo3"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ImageView
		android:id="@+id/callbutton_image_view"
		android:layout_width="456dp"
		android:layout_height="495dp"
		android:onClick="personalShopper"
		android:scaleType="centerCrop"
		android:src="@drawable/callbutton"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.501" />

	<ImageView
		android:id="@+id/shopbutton_image_view"
		android:layout_width="488dp"
		android:layout_height="495dp"
		android:layout_marginStart="410dp"
		android:onClick="shop"
		android:scaleType="centerCrop"
		android:src="@drawable/shopbutton"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="@+id/callbutton_image_view"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.501" />

	<ImageView
		android:id="@+id/lefttriangle_image_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:scaleType="centerCrop"
		android:src="@drawable/lefttriangle"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="-22dp"/>

	<ImageView
		android:id="@+id/bottomtriangle_image_view"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginTop="240dp"
		android:scaleType="centerCrop"
		android:src="@drawable/bottomtriangle"
		app:layout_constraintHorizontal_weight="1.05"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/ua_logo1_image_view" />

	<ImageView
		android:id="@+id/righttriangle_image_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:scaleType="centerCrop"
		android:src="@drawable/righttriangle"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ImageView
		android:id="@+id/toptriangle_image_view"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:scaleType="centerCrop"
		android:src="@drawable/toptriangle"
		app:layout_constraintHorizontal_weight="1"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="0dp"/>

	<ImageView
		android:id="@+id/logoua_image_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:alpha="0.96"
		android:scaleType="centerCrop"
		android:src="@drawable/logoua"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ImageView
		android:id="@+id/emailsignup_image_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:alpha="0.96"
		android:onClick="startAnimationOne"
		android:scaleType="center"
		android:src="@drawable/emailsignup"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>